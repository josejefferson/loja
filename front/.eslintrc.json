{
  "root": true,
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jquery": true
  },
  "plugins": [
    "only-warn"
  ],
  "ignorePatterns": [
    "*.min.js"
  ],
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "ecmaFeatures": {
      "modules": true
    },
    "allowImportExportEverywhere": true,
    "sourceType": "module"
  },
  "rules": {
    "react/display-name": "off",
    "react/no-children-prop": "off",
    "import/no-anonymous-default-export": [
      "off",
      {
        "allowArrowFunction": true
      }
    ],
    "indent": [
      "off",
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": "off",
    "quotes": [
      "warn",
      "single"
    ],
    "semi": [
      "warn",
      "never"
    ],
    "no-unused-vars": "off",
    "no-empty": [
      "warn",
      {
        "allowEmptyCatch": true
      }
    ],
    "no-undef": "off",
    "no-inner-declarations": "off",
    "camelcase": [
      "warn",
      {
        "properties": "always"
      }
    ],
    "comma-dangle": [
      "warn",
      "never"
    ],
    "jsx-quotes": [
      "warn",
      "prefer-double"
    ],
    "no-redeclare": "off"
  }
}